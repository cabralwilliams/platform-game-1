<svg version='1.1' xmlns="http://www.w3.org/2000/svg" viewBox="{{ initialX }} {{ initialY }} {{ screenWidth }} {{ screenHeight }}" id="gameScreen">
    {{#each backgroundGroups}}
        {{> background-group}}
    {{/each}}
    {{#each backgroundFixed }}
        {{> fixed-assets }}
    {{/each}}
    {{#each groundPaths }}
        {{> path-element }}
    {{/each}}
    {{#each character }}
        {{> simple-sprite }}
    {{/each}}
</svg>