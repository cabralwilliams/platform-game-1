<svg version='1.1' xmlns="http://www.w3.org/2000/svg" viewBox="{{ initialX }} {{ initialY }} {{ screenWidth }} {{ screenHeight }}" id="gameScreen">
    {{#each backgroundGroups}}
        {{> background-group}}
    {{/each}}
    {{#each backgroundFixed }}
        {{> fixed-assets }}
    {{/each}}
    {{#each groundPaths }}
        {{> path-element }}
    {{/each}}
    <g id="enemy0"></g>
    <g id="enemy1"></g>
    <g id="enemy2"></g>
    <g id="enemy3"></g>
    <g id="enemy4"></g>
    {{#each character }}
        {{> simple-sprite }}
    {{/each}}
    <g id="playerProjectile0"></g>
    <g id="playerProjectile1"></g>
    <g id="playerProjectile2"></g>
    <g id="playerProjectile3"></g>
    <g id="playerProjectile4"></g>
</svg>